#: data/civ1/units.ruleset:1207 data/civ2/units.ruleset:1904
#: data/classic/units.ruleset:1871 data/sandbox/units.ruleset:2174
#: data/civ2civ3/units.ruleset:2075 data/multiplayer/units.ruleset:1973
msgid "Carrier"
msgstr "Porta-Aviões"

#: data/civ1/units.ruleset:1235 data/civ2/units.ruleset:1932
#: data/classic/units.ruleset:1899 data/sandbox/units.ruleset:2205
#: data/civ2civ3/units.ruleset:2103 data/multiplayer/units.ruleset:2001
msgid "The Carrier is a mobile airport."
msgstr ""

#: data/civ1/units.ruleset:1237 data/civ2/units.ruleset:1934
